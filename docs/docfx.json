{
  "build": {
    "content": [
      {
        "files": [
          "articles/**.md", 
          "articles/**/toc.yml", 
          "toc.yml", 
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": ["2.*/**", "img/**", "files/**", "fonts/**", "css/**",
          "CNAME","images/**", "robots.txt"]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "NUnit Docs"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "custom_template"],
    "postProcessors": [ "ExtractSearchIndex" ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "sitemap": {
      "baseurl": "http://docs.nunit.org"
    }
  }
}
